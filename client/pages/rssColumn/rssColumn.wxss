
page {
  padding-top: 7%;
  background-color: rgb(64, 63, 60);
}

.one-rss {
  margin-top: 20rpx;
  background-color: rgb(73, 72, 69) !important;
}

.add-rss {
  position: fixed;
  bottom: 10rpx;
  right: 50%;
}

.rss-sources {
  margin-top: 20rpx;
}

.nav-class {
  margin: 0 20%;
  /*  z-index: 100;*/
  border-radius: 25rpx;
  border: 0rpx !important;
}

.tab-class {
  background-color: rgb(73, 72, 69) !important;
  color: white !important;
  border-radius: 25px 0px 0px 25px;
  border: 0rpx !important;
}

.tab-active-class {
  /*background-color: rgb(64, 63, 60);*/
  background-color: rgb(100, 99, 97) !important;
  color: white;
  border: 0rpx !important;
}

.van-tabs__scroll--card {
  border: 0px !important;
  background-color: rgb(64, 63, 60);
  border-radius: 0rpx !important;
}

.tab-view{
  margin-top: 120rpx;
}
.van-swipe-cell__left, .van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: orange;
}


