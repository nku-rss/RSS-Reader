page {
  /*BACKGROUND*/
  --grey-page-bk: rgb(64, 63, 60);
  --grey-tab: rgb(73, 72, 69);
  --grey-tab-active: rgb(100, 99, 97);
  --grey-tab-title: rgb(242, 240, 236);
  --grey-cell-bk:  rgb(73, 72, 69);
  --grey-cell-swipe-left: rgb(100, 99, 97);
  --grey-cell-swipe-right: rgb(100, 99, 97);
  --grey-tabbar-bk: rgb(64, 63, 60);
  --grey-tabbar-button-bk: rgb(64, 63, 60);
  /*TEXT*/
  --grey-cell-title: rgb(242, 240, 236);
  --grey-cell-label: rgb(202, 200, 196);
  --grey-cell-value: rgb(202, 200, 196);
  --grey-cell-swipe-title: rgb(202, 200, 196);
}

.page {
  padding-top: 3%;
  background-color: var(--grey-page-bk);
}

#skin-grey .nav-class {
  margin: 0 20%;
  z-index: 100;
  border-radius: 25rpx;
  border: 0rpx !important;
}

#skin-grey .cell-with-margin{
  margin-top:25rpx;
}

#skin-grey .tab-class {
  background-color: var(--grey-tab) !important;
  color: var(--grey-tab-title) !important;
  border-radius: 15rpx;
  border: 0rpx !important;
}

#skin-grey .tab-active-class {
  background-color: var(--grey-tab-active) !important;
  color: var(--grey-tab-title) !important;
  border: 0rpx !important;
}

#skin-grey .one-rss {
  background-color: var(--grey-cell-bk) !important;
}

#skin-grey .rss-posts {
  margin: 50rpx 0rpx 150rpx 0rpx;
}

#skin-grey .van-tabs__scroll--card {
  border: 0px !important;
  background-color: var(--grey-page-bk);
  border-radius: 0rpx !important;
}

#skin-grey .van-swipe-cell {
  border-radius: 25rpx;
}

#skin-grey .van-swipe-cell__left {
  display: inline-block;
  width: 65px;
  /*height: 44px;*/
  font-size: 15px;
  line-height: 44px;
  color: var(--grey-cell-swipe-title);
  text-align: center;
  background-color: var(--grey-cell-swipe-left);
}

#skin-grey .van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  /*height: 44px;*/
  font-size: 15px;
  line-height: 44px;
  color: var(--grey-cell-swipe-title);
  text-align: center;
  background-color: var(--grey-cell-swipe-right);
}

#skin-grey .add-rss {
  position: fixed;
  top: 18rpx;
  right: 20rpx;
  z-index: 100;
}

#skin-grey .add-rss-button {
  border: 0rpx !important;
}

#skin-grey .unread-star-buttons {
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0rpx;
  background-color: var(--grey-page-bk);
  width: 100%;
}

#skin-grey .tabbar-button {
  background-color: var(--grey-page-bk);
  border: 0px !important;
  color: var(--grey-cell-label);
}

button::after {
  border: none;
}

#skin-grey .button-hover {
  color: var(--grey-page-bk);
}

#skin-grey .tabbar-view-1 {
  position: absolute;
  left: 20%;
}

#skin-grey .tabbar-view-2 {
  position: relative;
  left: 50%;
  transform: translateX(-50%);
}

#skin-grey .tabbar-view-3 {
  position: absolute;
  right: 20%;
}


#skin-grey .van-cell__title {
  color: var(--grey-cell-title);
}

#skin-grey .van-cell__label {
  color: var(--grey-cell-label) !important;
}

#skin-grey .van-cell__value {
  color: var(--grey-cell-value) !important;
}


